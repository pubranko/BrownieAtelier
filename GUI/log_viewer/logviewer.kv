LogSelecter:    #LogViewerAppクラスからこれを呼び出す。ここから下の<LogSelecter>を呼び出している。

<LogSelecter>:  #ここからLogSelecterクラスを呼び出している。
    #canvas.before:
    canvas:
        Color:
            rgba: 0.0, 0.0, 0.0, 1
        Rectangle:  #長方形
            pos: self.pos
            size: self.size

    BoxLayout:
        # 検索条件ボックス
        #orientation: 'vertical'     # 'horizontal'だと横一列
        orientation: 'horizontal'     # 'horizontal'だと横一列
        size: root.size #大本のウィンドウのキャンバスに合わせる

        BoxLayout:
            padding: 20,30,20,10
            Label:
                id:start_time_from
                #font_size: 70
                #center_x: root.width / 4
                #top: root.top - 50
                text: "start_time_from"
            Label:
                id:start_time_to
                text: "start_time_to"
                font_size: 16 # フォントサイズ
                color: 1,1,0,1 # 文字色

                #pos: 1000, 240 # 位置
                size: cm(240), cm(240) # サイズ
                size_hint:1,0.5
                bold: True # 太字
                italic: True # イタリック
                #halign: "left"  # 水平揃え
                valign: "top" # 垂直揃え

